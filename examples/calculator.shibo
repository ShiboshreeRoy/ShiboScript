# Calculator Example
func add(a, b) {
    return a + b
}

func subtract(a, b) {
    return a - b
}

func multiply(a, b) {
    return a * b
}

func divide(a, b) {
    if (b == 0) {
        print("Error: Division by zero")
        return null
    }
    return a / b
}

# Test the calculator
print("Calculator Demo")
print("10 + 5 = " + add(10, 5))
print("10 - 5 = " + subtract(10, 5))
print("10 * 5 = " + multiply(10, 5))
print("10 / 5 = " + divide(10, 5))
print("10 / 0 = " + divide(10, 0))